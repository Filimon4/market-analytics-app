model User {
  id            BigInt     @id @default(autoincrement())
  name          String
  email         String
  roleId        Int
  password      String 
  createdAt     DateTime @default(now())
  statusId      Int

  role          UserRole   @relation(fields: [roleId], references: [id])
  status        UserStatus @relation(fields: [statusId], references: [id])

  strategies    Strategy[]
  apiKeys       ApiKey[]

  userSessions  UserSession[]
}