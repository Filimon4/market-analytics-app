model Channel {
  id           BigInt   @id @default(autoincrement())
  strategyId  BigInt
  periodStart DateTime
  periodEnd   DateTime
  trafficSource String
  budgetSpent Int
  impressions  Int
  clicks       Int
  leads        Int
  conversions  Int
  createdAt   DateTime
  updatedAt   DateTime

  strategy Strategy @relation(fields: [strategyId], references: [id])
  channelPerformances ChannelPerformance[]
}