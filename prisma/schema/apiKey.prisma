model ApiKey {
  id         BigInt   @id @default(autoincrement())
  createdBy BigInt
  name       String
  key        String
  permissions String
  createdAt DateTime
  updatedAt DateTime
  expiresAt DateTime
  statusId  Int

  creator User         @relation(fields: [createdBy], references: [id])
  status  ApiKeyStatus @relation(fields: [statusId], references: [id])
}